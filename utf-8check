import os

def is_utf8(filename):
    try:
        with open(filename, encoding='utf-8') as f:
            f.read()
        return True
    except UnicodeDecodeError:
        return False

for root, dirs, files in os.walk('.'):
    for filename in files:
        if not is_utf8(os.path.join(root, filename)):
            print(filename)